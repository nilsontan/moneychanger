<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <ui:with field="res" type="com.techstudio.erp.moneychanger.client.resources.Resources" />

  <g:AbsolutePanel width="160px" height="35px">
    <g:HorizontalPanel spacing="5">
      <g:Image resource="{res.iLogoPos}" />
      <g:HTMLPanel>
        <div style="height:5px" />
        <g:Label text="Moneychanger" addStyleNames="{res.pos.formTitle}" />
      </g:HTMLPanel>
    </g:HorizontalPanel>
  </g:AbsolutePanel>
</ui:UiBinder> 