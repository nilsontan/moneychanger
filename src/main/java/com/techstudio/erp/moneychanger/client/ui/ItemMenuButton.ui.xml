<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:with field="res" type="com.techstudio.erp.moneychanger.client.resources.Resources" />
  <ui:style>
    .centerSpan {
      text-align: center;
    }
  </ui:style>

  <g:HTMLPanel ui:field="pane" addStyleNames="gwt-Button itemMenuButton">
    <g:Image ui:field="image" />
    <g:Label ui:field="label" addStyleNames="{style.centerSpan}" />
  </g:HTMLPanel>
</ui:UiBinder> 